{
  "extends": ["prettier", "airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "env": {
    "commonjs": true,
    "node": true
  },
  "rules": {
    "no-unused-vars": 0,
    "no-shadow": 0,
    "no-console": 0,
    "indent": 0,
    "linebreak-style": 0,
    "consistent-return": 0,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 100,
        "tabWidth": 4,
        "semi": false
      }
    ],
    "prettier/prettier": 0
  },
  "plugins": ["prettier"]
}
